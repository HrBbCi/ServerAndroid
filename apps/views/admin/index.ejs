                    </div>
                    <!-- ./col -->
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>News</h3>
                            </div>

                            <a target="_blank"  href="/admin/news" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6" id = "home_employee" >
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Employee</h3>
                            </div>

                            <a target="_blank"  href="/admin/employee" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Notification</h3>
                            </div>

                            <a target="_blank" href="/admin/notification" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
					<div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Customer</h3>
                            </div>

                            <a target="_blank" href="/admin/customer" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6" id = "home_statistical">
                            <!-- small box -->
                            <div class="small-box bg-aqua">
                                <div class="inner" style="text-align: center">
                                    <h3>Statistical</h3>
                                </div>
    
                                <a  target="_blank" href="/admin/statistical" class="small-box-footer">More
                                    info <i class="fa fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div>
                </div>
