<%- include header %>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <%- include mainheader %>
        <%- include sidebar %>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>Dashboard</h1>
                <ol class="breadcrumb">
                    <li><a href="/admin"><i class="fa fa-dashboard"></i>
                            Home</a></li>
                    <li class="active">Dashboard</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">
                <!-- Small boxes (Stat box) -->
                <div class="row">
                    <!-- ./col -->
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Order</h3>
                            </div>

                            <a href="/admin/order" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Product</h3>
                            </div>

                            <a href="/admin/product" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Category</h3>
                            </div>

                            <a href="/admin/category" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <!-- ./col -->
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>News</h3>
                            </div>

                            <a href="/admin/news" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6">
                        <!-- small box -->
                        <div class="small-box bg-aqua">
                            <div class="inner" style="text-align: center">
                                <h3>Employee</h3>
                            </div>

                            <a href="/admin/employee" class="small-box-footer">More
                                info <i class="fa fa-arrow-circle-right"></i>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-xs-6">
                            <!-- small box -->
                            <div class="small-box bg-aqua">
                                <div class="inner" style="text-align: center">
                                    <h3>Notification</h3>
                                </div>
    
                                <a href="/admin/notification" class="small-box-footer">More
                                    info <i class="fa fa-arrow-circle-right"></i>
                                </a>
                            </div>
                        </div>
                </div>
                <!-- /.row -->
            </section>
            <!-- Main row -->
        </div>
    </div>
    <!-- /.content-wrapper -->
    <%- include footer %>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="/static/resources/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="/static/resources/bower_components/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script src="/static/resources/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="/static/resources/bower_components/raphael/raphael.min.js"></script>
    <script src="/static/resources/bower_components/morris.js/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="/static/resources/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="/static/resources/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="/static/resources/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="/static/resources/bower_components/jquery-knob/dist/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="/static/resources/bower_components/moment/min/moment.min.js"></script>
    <script src="/static/resources/bower_components/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="/static/resources/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="/static/resources/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="/static/resources/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="/static/resources/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="/static/resources/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="/static/resources/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/static/resources/dist/js/demo.js"></script>

</body>

</html>